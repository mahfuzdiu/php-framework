{
  "name": "vendorname/custom-framework",
  "description": "A custom PHP framework",
  "type": "project",
  "require": {
    "php": "^8.1",
    "php-di/php-di": "^7.0",
    "respect/validation": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
